{
    "name": "shippinno/job",
    "description": "A package for creating, serializing, storing, queueing, and running jobs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hirofumi Tanigami",
            "email": "hirofumi.tanigami@shippinno.co.jp",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shippinno\\Job\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shippinno\\Job\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-pcntl": "*",
        "doctrine/orm": "^2.5.0",
        "enqueue/enqueue": "^0.10.14",
        "jms/serializer": "^1.11.0",
        "shippinno/sqs": "^1.0",
        "symfony/yaml": "^5.1.0"
    },
    "require-dev": {
        "illuminate/console": "^5.5",
        "illuminate/container": "^5.5",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2",
        "jeroen/psr-log-test-doubles": "^2.2|^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
